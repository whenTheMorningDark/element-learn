{
  "name": "learn-element",
  "version": "1.0.0",
  "description": "base",
  "author": {
    "name": "kafei",
    "email": "404792402@qq.com"
  },
  "main": "lib/t-ui-plus.umd.cjs",
  "module": "lib/t-ui-plus.es.cjs",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git"
  },
  "scripts": {
    "dev": "vite",
    "build:stage": "vue-tsc --noEmit && vite build --mode staging",
    "build:prod": "vue-tsc --noEmit && vite build",
    "preview:stage": "pnpm build:stage && vite preview",
    "preview:prod": "pnpm build:prod && vite preview",
    "lint:eslint": "eslint --cache --max-warnings 0 \"{src,tests,types}/**/*.{vue,js,jsx,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write \"{src,tests,types}/**/*.{vue,js,jsx,ts,tsx,json,css,less,scss,html,md}\"",
    "lint": "pnpm lint:eslint && pnpm lint:prettier",
    "prepare": "husky install",
    "test": "vitest",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "lib": "rimraf lib && vite build"
  },
  "dependencies": {
    "@element-plus/icons-vue": "2.1.0",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "unplugin-vue-components": "^0.25.2",
    "vitepress": "1.0.0-alpha.75",
    "axios": "1.4.0",
    "dayjs": "1.11.8",
    "element-plus": "2.3.7",
    "js-cookie": "3.0.5",
    "lodash-es": "4.17.21",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "path-browserify": "1.0.1",
    "path-to-regexp": "6.2.1",
    "pinia": "2.1.4",
